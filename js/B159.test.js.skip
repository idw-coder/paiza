const { calculateSatisfaction } = require('./B159.js');

test('calculateSatisfaction function basic test', () => {
  const testArray = [
    [10, 2, 1],
    [40, 5, 2],
    [70, 8, 3],
    [50, 6, 3]
  ];

  const result = calculateSatisfaction(testArray, 0, 1, 3, 100);
  
  expect(result).toBe(13);
});